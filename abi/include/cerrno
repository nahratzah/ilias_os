#ifndef _CERRNO_
#define _CERRNO_

#include <abi/abi.h>
#include <abi/errno.h>

_namespace_begin(std)
using abi::errno;
_namespace_end(std)
#if !defined(_TEST) && defined(__cplusplus)
/*
 * We won't pollute the global namespace with errno, in the testing case.
 */
using std::errno;
#endif

#ifndef _TEST

#define E2BIG			_ABI_E2BIG
#define EACCES			_ABI_EACCES
#define EADDRINUSE		_ABI_EADDRINUSE
#define EADDRNOTAVAIL		_ABI_EADDRNOTAVAIL
