#define ENOPROTOOPT		_ABI_ENOPROTOOPT
#define ENOSPC			_ABI_ENOSPC
#define ENOSR			_ABI_ENOSR
#define ENOSTR			_ABI_ENOSTR
#define ENOSYS			_ABI_ENOSYS
#define ENOTCONN		_ABI_ENOTCONN
#define ENOTDIR			_ABI_ENOTDIR
#define ENOTEMPTY		_ABI_ENOTEMPTY
#define ENOTRECOVERABLE		_ABI_ENOTRECOVERABLE
#define ENOTSOCK		_ABI_ENOTSOCK
#define ENOTSUP			_ABI_ENOTSUP
#define ENOTTY			_ABI_ENOTTY
#define ENXIO			_ABI_ENXIO
#define EOPNOTSUPP		_ABI_EOPNOTSUPP
#define EOVERFLOW		_ABI_EOVERFLOW
