#define ECONNREFUSED		_ABI_ECONNREFUSED
#define ECONNRESET		_ABI_ECONNRESET
#define EDEADLK			_ABI_EDEADLK
#define EDESTADDRREQ		_ABI_EDESTADDRREQ
#define EDOM			_ABI_EDOM
#define EDQUOT			_ABI_EDQUOT
#define EEXIST			_ABI_EEXIST
#define EFAULT			_ABI_EFAULT
#define EFBIG			_ABI_EFBIG
#define EHOSTUNREACH		_ABI_EHOSTUNREACH
#define EIDRM			_ABI_EIDRM
#define EILSEQ			_ABI_EILSEQ
#define EINPROGRESS		_ABI_EINPROGRESS
#define EINTR			_ABI_EINTR
#define EINVAL			_ABI_EINVAL
#define EIO			_ABI_EIO
#define EISCONN			_ABI_EISCONN
#define EISDIR			_ABI_EISDIR
#define ELOOP			_ABI_ELOOP
#define EMFILE			_ABI_EMFILE
#define EMLINK			_ABI_EMLINK
#define EMSGSIZE		_ABI_EMSGSIZE
#define EMULTIHOP		_ABI_EMULTIHOP
#define ENAMETOOLONG		_ABI_ENAMETOOLONG
#define ENETDOWN		_ABI_ENETDOWN
#define ENETRESET		_ABI_ENETRESET
#define ENETUNREACH		_ABI_ENETUNREACH
#define ENFILE			_ABI_ENFILE
#define ENOBUFS			_ABI_ENOBUFS
#define ENODATA			_ABI_ENODATA
#define ENODEV			_ABI_ENODEV
#define ENOENT			_ABI_ENOENT
